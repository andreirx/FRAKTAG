{
  "name": "@fraktag/functions",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/*.ts --bundle --platform=node --target=node20 --outdir=dist --external:@aws-sdk/*",
    "build:worker": "esbuild src/ingestion-worker.ts --bundle --platform=node --target=node20 --outfile=dist/ingestion-worker.js --external:@aws-sdk/*"
  },
  "dependencies": {
    "@fraktag/api": "workspace:*",
    "@fraktag/engine": "workspace:*",
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/lib-dynamodb": "^3.500.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/client-ses": "^3.500.0",
    "@aws-sdk/client-secrets-manager": "^3.500.0",
    "aws-jwt-verify": "^4.0.0",
    "serverless-http": "^3.2.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.3"
  }
}
